sp {propose*initialize-monety
   (state <s> ^superstate nil
             -^name)
-->
   (<s> ^operator <o> +)
   (<o> ^name initialize-monety)
}

sp {apply*initialize-monety
   (state <s> ^operator <op>)
   (<op> ^name initialize-monety)
-->
   (<s> ^name monety
        ^pig <p1>
        ^pig <p2>)
   
   (<p1> ^capacity 50
         ^content 0
         ^namep big)
   
   (<p2> ^capacity 30
         ^content 0
         ^namep small)

}

